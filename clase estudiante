class estudiante:
    def __init__(self,nombre,edad):
        self.__nombre=nombre
        self.__edad=edad
        self.__notas=[]

    def get_nombre(self):
        return self.__nombre

    def get_edad(self):
        return self.__edad

    def get_notas(self):
        return self.__notas

    def agregar_nota(self,x):
        self.__notas.append(x)

    def promedio(self):
        if len(self.__notas)==0:
            return 0
        return sum(self.__notas)/len(self.__notas)



    
