class libro:
def __init__(self,titulo,autor,año):
self.__titulo=titulo
self.__autor=autor
self.__año=año

def get_titulo(self):
return self.__titulo

def get_autor(self):
return self.__autor

def get_año(self):
return self.__año

def descripcion(self):
return "titulo:"+self.__titulo+" autor:"+self.__autor+" año:"+str(self.__año)

def es_clasico(self,actual):
return actual-self.__año
>50
