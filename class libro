class Libro:
    def __init__(self, titulo, autor, año_publicacion):
        self.__titulo = titulo
        self.__autor = autor
        self.__año_publicacion = año_publicacion

    def descripcion(self):
        return f"'{self.__titulo}' de {self.__autor} ({self.__año_publicacion})"

    def es_clasico(self):
        return 2025 - self.__año_publicacion > 50
