class libro:
def __init__(self,titulo,autor,anio):
self.__titulo=titulo
self.__autor=autor
self.__anio=anio

def get_titulo(self):
return self.__titulo

def get_autor(self):
return self.__autor

def get_anio(self):
return self.__anio

def descripcion(self):
return "titulo:"+self.__titulo+" autor:"+self.__autor+" anio:"+str(self.__anio)

def es_clasico(self,actual):
return actual-self.__anio>50
